<!DOCTYPE html>
<html>
<head>
  <title><%= controller.controller_name %>: <%= controller.action_name %></title>
  <%= stylesheet_link_tag 'scaffold', 'main' %>
  <%= stylesheet_link_tag 'iphone', :media => 'only screen and (max-device-width: 480px)' %>
  
  <%#= javascript_include_tag 'jquery-1.4.2.min', 'application' %>
  <%= javascript_include_tag :defaults %>
  <%- if protect_against_forgery? -%>
    <meta name="authenticity-token" id="authenticity-token" content="<%=h form_authenticity_token %>">
  <%- end -%>
  <%= csrf_meta_tag %>
</head>
<body>

<p style="color: green"><%= flash[:notice] %></p>

<%= yield %>

</body>
</html>
